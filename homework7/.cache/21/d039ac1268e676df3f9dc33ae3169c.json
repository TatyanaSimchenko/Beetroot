scss","name":null,"original":{"line":16,"column":14},"generated":{"line":157,"column":15}}],"sources":{"scss\\style.scss":"@import url('https://fonts.googleapis.com/css2?family=Open+Sans:wght@700&family=Playfair+Display&display=swap');\r\n\r\n\r\n@import './vars';\r\n@import './mixins';\r\n@import './common';\r\n\r\n//components\r\n@import './components/container';\r\n@import './components/header';\r\n@import './components/nav';\r\n@import './components/button';\r\n@import './components/icon';\r\n@import './components/item-button';\r\n@import './components/main';\r\n@import './components/article';\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n// @font-face {\r\n//   font-family: 'ProximaNova';\r\n//   src: url('./fonts/ProximaNovaRegular.woff') format('woff'),\r\n//        url('./fonts/ProximaNovaRegular.woff2') format('woff2');\r\n//   font-weight: 400;\r\n//   font-style: normal;\r\n// }\r\n\r\n\r\n// font-family: 'Open Sans', sans-serif;\r\n// font-family: 'Playfair Display', serif;","scss\\_common.scss":"* {\r\n  box-sizing: border-box;\r\n  margin: 0;\r\n  padding: 0;\r\n}\r\n\r\nbody {\r\n  width: 1200px;\r\n  background-color: $BACKGROUND-COLOR;\r\n  margin-left: auto;\r\n  margin-right: auto;\r\n  font-size: $base-font-size;\r\n  font-family: $base-font;\r\n  \r\n  min-height: 100vh;\r\n}","scss\\_vars.scss":"\r\n//colors\r\n$WHITE: #fff;\r\n$BLACK: #000;\r\n$BACKGROUND-COLOR: #464f54;\r\n$YELLOW: #f7bd2c;\r\n$SUBTITLE-COLOR: #b4b4b4;\r\n$TOP-SUBTITLE-COLOR: #767676;\r\n$BOTTOM-SUBTITLE-COLOR: #8f8f8f;\r\n$GORIZONT-LINE-COLOR: #303030;\r\n$VERTICAL-LINE-COLOR: #e8eced;\r\n\r\n\r\n//fonts\r\n$base-font: 'Open Sans', sans-serif;\r\n$second-font: 'Playfair Display', serif;\r\n$base-font-size: 16px;\r\n\r\n//font-sizes\r\n@function calcRem($size) {\r\n  $remSize: $size / $base-font-size;\r\n  @return #{$remSize}rem;\r\n}\r\n\r\n$H15: calcRem(15px);\r\n$H20: calcRem(20px);\r\n$H27: calcRem(27px);\r\n$H30: calcRem(30px);\r\n$H48: calcRem(48px);\r\n$H78: calcRem(78px);","scss\\components\\_container.scss":".container {\r\n  // max-width: 1200px;\r\n  // width: 100%;\r\n  background-color: $BACKGROUND-COLOR;\r\n  // margin-left: auto;\r\n  // margin-right: auto;\r\n}\r\n","scss\\components\\_header.scss":".header {\r\n  @include flex-col(column, flex-end, stretch, nowrap);\r\n  background-color: $BLACK;\r\n  max-width: 880px;\r\n  min-height: 190px;\r\n  padding-left: 65px;\r\n  padding-top: 50px;\r\n  padding-bottom: 50px;\r\n  margin-top: 46px;\r\n  margin-left: auto;\r\n  &__subtitle {\r\n    font-family: $base-font;\r\n    @include text($H15, 400, $TOP-SUBTITLE-COLOR);\r\n    text-transform: uppercase;\r\n  }\r\n  &__title {\r\n    font-family: $second-font;\r\n    @include text($H48, 400, $WHITE);\r\n    position: relative;\r\n    &::after {\r\n      content: \"\";\r\n      width: 1px;\r\n      height: 40px;\r\n      background-color: $WHITE;\r\n      position: absolute;\r\n      top: 13px;\r\n      margin-left: 8px;\r\n    }\r\n  }\r\n\r\n  &__line {\r\n    // width: 100%;\r\n    // max-width: 815px;\r\n    width: 815px;\r\n    height: 2px;\r\n    background-color: $GORIZONT-LINE-COLOR;\r\n    @include flex(row, flex-end, stretch, nowrap);\r\n  }\r\n}\r\n","scss\\_mixins.scss":"@mixin text($font-size: $H27, $font-weight: 400, $font-color: $BLACK) {\r\n  font-size: $font-size;\r\n  font-weight: $font-weight;\r\n  color: $font-color;\r\n}\r\n\r\n@mixin flex($direction: row, $jc: flex-start, $ai: stretch, $wrap: nowrap) {\r\n  display: flex;\r\n  flex-direction: $direction;\r\n  justify-content: $jc;\r\n  align-items: $ai;\r\n  flex-wrap: $wrap;\r\n}\r\n\r\n@mixin flex-col ($direction: column, $jc: flex-start, $ai: stretch, $wrap: nowrap) {\r\n  display: flex;\r\n  flex-direction: $direction;\r\n  justify-content: $jc;\r\n  align-items: $ai;\r\n  flex-wrap: $wrap;\r\n}\r\n\r\n","scss\\components\\_nav.scss":".nav {\r\n  @include flex(row, flex-start,stretch, nowrap);\r\n  margin-left: auto;\r\n  list-style: none;\r\n  background-color: $YELLOW;\r\n  max-width: 813px;\r\n  padding: 22px 40px;\r\n}\r\n","scss\\components\\_button.scss":".button {\r\n  background-color: transparent;\r\n  border: none;\r\n  width: 40px;\r\n  height: 40px;\r\n  margin-right: 15px;\r\n  cursor: pointer;\r\n  &:active, &:focus  {\r\n    outline: none;\r\n  }\r\n  \r\n\r\n  \r\n}","scss\\components\\_icon.scss":".icon {\r\n  width: 35px;\r\n  height: 35px;\r\n  &:hover {\r\n    fill: $WHITE;\r\n  }\r\n}\r\n\r\n","scss\\components\\_main.scss":".main {\r\n  max-width: 1060px;\r\n  background-color: $WHITE;\r\n  padding: 200px 130px 20px 130px;\r\n  position: absolute;\r\n  top: 140px;\r\n  z-index: -1;\r\n  &__line {\r\n    width: 1px;\r\n    height: 100%;\r\n    border: 1px solid $VERTICAL-LINE-COLOR;\r\n    position: absolute;\r\n    left: 70px;\r\n    top: 0;\r\n    color: $VERTICAL-LINE-COLOR;\r\n    @include flex(row, flex-start, stretch, nowrap);\r\n  }\r\n}\r\n","scss\\components\\_article.scss":".article {\r\n  &__subtitle {\r\n    @include text($H20, 400, $SUBTITLE-COLOR);\r\n    text-transform: uppercase;\r\n    margin-bottom: 44px;\r\n  }\r\n  &__title {\r\n    @include text($H78, 400, $BLACK);\r\n    text-transform: uppercase;\r\n  }\r\n  &__title-text{\r\n    @include text($H30, 400, $BOTTOM-SUBTITLE-COLOR);\r\n    font-style: italic;\r\n    font-family: $second-font;\r\n    margin-bottom: 60px;\r\n  }\r\n  &__text {\r\n    @include text($H27, 400, $BLACK);\r\n    font-family: $second-font;\r\n    padding-bottom: 40px;\r\n    &-italic {\r\n      font-style: italic;\r\n      font-family: $second-font;\r\n    }\r\n  }\r\n}"},"lineCount":null}},"error":null,"hash":"e6f18eed7fce90cc4243eed138e97f5b","cacheData":{"env":{}}}